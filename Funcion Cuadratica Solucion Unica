#include <iostream>

using namespace std;

int main()
{
    int f, a, b, c, x;
    cout<<"Ingrese el valor de a: ";
    cin>>a;
    cout<<"Ingrese el valor de b: ";
    cin>>b;
    cout<<"Ingrese el valor de c: ";
    cin>>c;
    if(a==0){
        cout<<"Solucion Indeterminada";
    }else{
        f=b*b-4*a*c;
        if(f==0){
            x=-b/(2*a);
            cout<<"La solucion unica de X es: "<<x;
        }else{
            cout<<"NO existe una unica solucion";
        }
    }

    return 0;
}
